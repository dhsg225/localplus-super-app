{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "..",
    "baseUrl": "..",
    "paths": {
      "@/*": ["./partner/src/*"],
      "shared/*": ["./shared/*"]
    },
    "composite": true,
    "jsx": "react-jsx",
    "types": ["vite/client"]
  },
  "include": [
    "partner/src",
    "shared"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "admin",
    "src"
  ]
}
